NGINX_ERROR_LOG_PATTERN (?<Time>\d{4}/\d{2}/\d{2} \d{2}:\d{2}:\d{2}) \[%{DATA:nginx_severity}\] %{NOTSPACE} %{NOTSPACE} (?<nginx_message>(.|\r|\n)*)(?:, client: (?<nginx_client_ip>%{IP}|%{HOSTNAME}))(?:, server: %{IPORHOST:nginx_server})(?:, request: \"%{WORD:Verb} %{URIPATHPARAM:Request} HTTP/%{NUMBER:HttpVersion}\")?(?:, upstream: \"%{DATA:Upstream}\")?(?:, host: \"%{IPORHOST:nginx_host}\")?(?:, referrer: \"%{URI:nginx_referrer}\")?
NGINX_ERROR_LOG_PATTERN_V2 (?<Time>\d{4}/\d{2}/\d{2} \d{2}:\d{2}:\d{2}) \[%{LOGLEVEL:LogLevel}\] %{NOTSPACE} %{NOTSPACE} (?<ErrorMessage>(.|\r|\n)*)(?:, client: (?<ClientIP>%{IP}|%{HOSTNAME}))(?:, server: %{IPORHOST:Server})(?:, request: \"%{WORD:Verb} %{URIPATHPARAM:Request} HTTP/%{NUMBER:HttpVersion}\")?(?:, upstream: \"%{DATA:Upstream}\")?(?:, host: \"%{IPORHOST:Host}\")?(?:, referrer: \"%{URI:Referrer}\")?
NGINX_ERROR_LOG_PATTERN_V3 (?<Time>\d{4}/\d{2}/\d{2} \d{2}:\d{2}:\d{2}) \[%{LOGLEVEL:LogLevel}\] %{POSINT:PID}#%{NUMBER:ThreadID}\: \*%{NUMBER:ConnectionId} (?<ErrorMessage>(.|\r|\n)*)(?:, client: (?<ClientIP>%{IP}|%{HOSTNAME}))(?:, server: %{URIHOST:Server})(?:, request: \"%{WORD:Verb} %{URIPATHPARAM:Request} HTTP/%{NUMBER:HttpVersion}\")?(?:, upstream: \"%{DATA:Upstream}\")?(?:, host: \"%{IPORHOST:Host}\")?(?:, referrer: \"%{URI:Referrer}\")?
NGINX_ERROR_LOG_PATTERN_V4 (?<Time>%{YEAR}[./]%{MONTHNUM}[./]%{MONTHDAY} %{TIME}) \[%{LOGLEVEL:LogLevel}\] %{POSINT:PID}#%{NUMBER:ThreadID}\: \*%{NUMBER:ConnectionId} (?<ErrorMessage>(.|\r|\n)*)(?:, client: (?<ClientIP>%{IP}|%{HOSTNAME}))(?:, server: %{URIHOST:Server})(?:, request: \"%{WORD:Verb} %{URIPATHPARAM:Request} HTTP/%{NUMBER:HttpVersion}\")?(?:, upstream: \"%{DATA:Upstream}\")?(?:, host: \"%{IPORHOST:Host}\")?(?:, referrer: \"%{URI:Referrer}\")?
NGINX_ACCESS_LOG_PATTERN_V1 %{IPORHOST:IP} - %{DATA:UserName} \[%{HTTPDATE:HttpTime}\] \"%{WORD:Method} %{DATA:URL} HTTP/%{NUMBER:HttpVersion}\" %{NUMBER:ResponseCode} %{NUMBER:Bytes} \"%{DATA:Referrer}\" \"%{DATA:Agent}\"
NGINX_ACCESS_LOG_PATTERN_V2 %{IPORHOST:ClientIP} - %{DATA:UserName} \[%{HTTPDATE:LogTime}\] \"%{WORD:Method} %{DATA:RequestURL} HTTP/%{NUMBER:HttpVersion}\" %{NUMBER:ResponseCode} %{NUMBER:Bytes} \"%{DATA:Referrer}\" \"%{DATA:Agent}\"